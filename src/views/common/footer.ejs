<script>
    $(document).ready(function () {
        var max_fields = 30;
        var wrapper = $(".container1");
        var add_button = $(".add_form_field");
        var val = $(".add_form_field").data('id');

        var x = val - 1;
        $(add_button).click(function (e) {
            e.preventDefault();
            if (x < max_fields) {
                x++;
                $(wrapper).append(`<div class="row mt-3"><div class="col-md-5"> <label class="form-label">Name</label><input type="text" name="field[name][]" class="form-control"  placeholder="Name" required><div class="invalid-feedback"> Please enter name. </div></div><div class="col-md-5"> <label class="form-label">Value</label><input type="text" name="field[value][]" required class="form-control"  placeholder="Value"><div class="invalid-feedback"> Please enter value. </div></div>
<div class="col-md-2"><label class="form-label">.</label><a href="#" class="btn btn-danger delete">Delete</a></div></div>`); //add input box
            } else {
                alert('You Reached the limits')
            }
        });

        $(wrapper).on("click", ".delete", function (e) {
            e.preventDefault();
            $(this).parent('div').parent('div').remove();
            x--;
        })
    });
</script>

