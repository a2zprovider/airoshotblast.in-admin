<%- include('../../common/header'); -%>

    <!-- main-content -->
    <div class="main-content app-content">
        <!-- container -->
        <div class="main-container container-fluid">
            <!-- breadcrumb -->
            <div class="breadcrumb-header justify-content-between">
                <div class="my-auto">
                    <div class="d-flex">
                        <h4 class="content-title mb-0 my-auto">Edit Setting</h4><span
                            class="text-muted mt-1 tx-13 ms-2 mb-0">/
                            Setting</span>
                    </div>
                </div>
                <div class="d-flex my-xl-auto right-content">

                    <div class="mb-xl-0">
                        <a href="/setting" class="btn btn-primary">View Setting</a>
                    </div>
                </div>
            </div>
            <!-- breadcrumb -->
            <div>
                <!-- success -->
                <% if(success !='' ){ %>
                    <div class="alert alert-success" role="alert">
                        <%= success %>
                    </div>
                    <% } %>

                        <!-- errors  -->
                        <% if(danger !='' ){ %>
                            <div class="alert alert-danger" role="alert">
                                <%= danger %>
                            </div>
                            <% } %>

                                <!-- warning -->
                                <% if(info !='' ){ %>
                                    <div class="alert alert-info" role="alert">
                                        <%= info %>
                                    </div>
                                    <% } %>
            </div>
            <!--Row-->
            <div class="row">
                <div class="col-lg-12 col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">Edit Setting</h3>
                        </div>
                        <div class="card-body">
                            <form class="row g-3 needs-validation" novalidate=""
                                action="<%= `/setting/update/${list.id}` %>" method="POST"
                                enctype="multipart/form-data">
                                <div class="col-md-6">
                                    <label for="title" class="form-label">Title</label>
                                    <input type="text" name="title" value="<%= list.title %>" class="form-control"
                                        id="title" placeholder="Title" required="">
                                    <div class="invalid-feedback"> Please enter title. </div>
                                </div>
                                <div class="col-md-6">
                                    <label for="tagline" class="form-label">Tagline</label>
                                    <input type="text" name="tagline" value="<%= list.tagline %>" class="form-control"
                                        id="tagline" placeholder="Tagline">
                                    <div class="invalid-feedback"> Please enter tagline. </div>
                                </div>
                                <div class="col-md-6">
                                    <label for="email" class="form-label">Email</label>
                                    <input type="text" name="email" value="<%= list.email %>" class="form-control"
                                        id="email" placeholder="Email" required>
                                    <div class="invalid-feedback"> Please enter email. </div>
                                </div>
                                <div class="col-md-6">
                                    <label for="mobile" class="form-label">Mobile</label>
                                    <input type="text" name="mobile" value="<%= list.mobile %>" class="form-control"
                                        id="mobile" placeholder="Mobile" required>
                                    <div class="invalid-feedback"> Please enter mobile. </div>
                                </div>

                                <div class="col-md-3">
                                    <label for="logo" class="form-label">Logo</label>
                                    <input type="file" name="logo" class="form-control" id="logo">
                                    <div class="invalid-feedback"> Please select logo. </div>
                                    <% if(list.logo){ %>
                                        <br>
                                        <div
                                            style="border: 1px solid #ccc;width: 50px;padding: 3px; margin-right: 10px; position: relative;">
                                            <img src="upload/setting/logo/<%= list.logo %>" width="50px"
                                                alt="<%= list.title %>">
                                        </div>
                                        <% } %>
                                </div>
                                <div class="col-md-3">
                                    <label for="logo2" class="form-label">Logo 2</label>
                                    <input type="file" name="logo2" class="form-control" id="logo2">
                                    <div class="invalid-feedback"> Please select logo2. </div>
                                    <% if(list.logo2){ %>
                                        <br>
                                        <div
                                            style="border: 1px solid #ccc;width: 50px;padding: 3px; margin-right: 10px; position: relative;">
                                            <img src="upload/setting/logo2/<%= list.logo2 %>" width="50px"
                                                alt="<%= list.title %>">
                                        </div>
                                        <% } %>
                                </div>
                                <div class="col-md-3">
                                    <label for="favicon" class="form-label">Favicon</label>
                                    <input type="file" name="favicon" class="form-control" id="favicon">
                                    <div class="invalid-feedback"> Please select favicon. </div>
                                    <% if(list.favicon){ %>
                                        <br>
                                        <div
                                            style="border: 1px solid #ccc;width: 50px;padding: 3px; margin-right: 10px; position: relative;">
                                            <img src="upload/setting/favicon/<%= list.favicon %>" width="50px"
                                                alt="<%= list.title %>">
                                        </div>
                                        <% } %>
                                </div>
